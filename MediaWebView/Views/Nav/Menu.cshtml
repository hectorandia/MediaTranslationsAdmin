@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Customer", null,
    new { @class = "btn btn-block btn-default btn-lg"})


@foreach(var link in Model)
{
    @Html.RouteLink(link, new
    {
    controller = "Customer",
    action = "List",
    city = link,
    page = 1
    }, new
        {
            @class = "btn btn-block btn-default btn-lg" + (link == ViewBag.SelectedCity ? "btn-primary": "")
        })
}